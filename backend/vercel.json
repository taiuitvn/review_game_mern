{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/auth/register",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}